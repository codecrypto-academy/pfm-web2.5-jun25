{
  "name": "besu-network-manager",
  "version": "1.0.0",
  "description": "TypeScript library for managing Hyperledger Besu networks with Clique consensus using Docker",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "besu",
    "blockchain",
    "ethereum",
    "clique",
    "consensus",
    "docker",
    "network",
    "hyperledger",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "dockerode": "^4.0.7",
    "elliptic": "^6.5.4",
    "js-sha3": "^0.9.3",
    "keccak": "^3.0.4"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.23",
    "@types/keccak": "^3.0.5",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "tsx": "^4.15.7",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

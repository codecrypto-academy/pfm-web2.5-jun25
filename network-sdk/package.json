{
  "name": "network-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Besu network operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "./build.sh",
    "build:tsc": "tsc",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "network:setup": "node network-cli.mjs setup",
    "network:start": "node network-cli.mjs start",
    "network:stop": "node network-cli.mjs stop",
    "network:restart": "node network-cli.mjs restart",
    "network:status": "node network-cli.mjs status",
    "network:test": "node network-cli.mjs test",
    "network:logs": "node network-cli.mjs logs",
    "network:reset": "node network-cli.mjs reset",
    "network:help": "node scripts/network-cli.mjs help"
  },
  "keywords": [
    "besu",
    "blockchain",
    "network",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/elliptic": "^6.4.18",
    "elliptic": "^6.5.4",
    "keccak256": "^1.0.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
